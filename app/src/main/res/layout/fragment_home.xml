<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".mainNavigation.Fragments.HomeFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d2fcfa">

    <ImageView
        android:id="@+id/line1"
        android:layout_width="175dp"
        android:layout_height="92dp"
        android:layout_marginBottom="96dp"
        android:src="@drawable/line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_editor_absoluteY="600dp" />

    <ImageView
        android:id="@+id/selfAwareness"
        android:layout_width="118dp"
        android:layout_height="76dp"
        android:layout_marginStart="28dp"
        android:src="@drawable/self_awarness_icon"
        app:layout_constraintBottom_toTopOf="@+id/line1"
        app:layout_constraintStart_toEndOf="@+id/clouds" />

    <ImageView
        android:id="@+id/clouds"
        android:layout_width="73dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/clouds"
        app:layout_constraintBottom_toBottomOf="@+id/selfAwareness"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/tree"
        android:layout_width="62dp"
        android:layout_height="76dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/tree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lines" />

    <ImageView
        android:id="@+id/mindIcon"
        android:layout_width="155dp"
        android:layout_height="86dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="60dp"
        android:src="@drawable/mind_icon"
        app:layout_constraintEnd_toStartOf="@+id/lines"
        app:layout_constraintTop_toBottomOf="@+id/mountains" />

    <ImageView
        android:id="@+id/lines"
        android:layout_width="32dp"
        android:layout_height="29dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="56dp"
        android:src="@drawable/lines"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mountains" />

    <ImageView
        android:id="@+id/line2"
        android:layout_width="70dp"
        android:layout_height="104dp"
        android:layout_marginStart="69dp"
        android:layout_marginBottom="112dp"
        android:src="@drawable/line2"
        app:layout_constraintBottom_toTopOf="@+id/clouds"
        app:layout_constraintEnd_toStartOf="@+id/mindIcon"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/line3"
        android:layout_width="61dp"
        android:layout_height="48dp"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/line3"
        app:layout_constraintBottom_toTopOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/mountains"
        android:layout_width="221dp"
        android:layout_height="155dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="84dp"
        android:src="@drawable/mountains"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/line3"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line5"
        android:layout_width="95dp"
        android:layout_height="3dp"
        android:layout_marginBottom="24dp"
        android:background="#1f314a"
        app:layout_constraintBottom_toBottomOf="@+id/mindIcon"
        app:layout_constraintEnd_toStartOf="@+id/line6" />

    <ImageView
        android:id="@+id/line6"
        android:layout_width="46dp"
        android:layout_height="146dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="8dp"
        android:rotation="180"
        android:src="@drawable/line2"
        app:layout_constraintEnd_toStartOf="@+id/tree"
        app:layout_constraintTop_toBottomOf="@+id/mountains" />

    <ImageView
        android:id="@+id/line7"
        android:layout_width="30dp"
        android:layout_height="140dp"
        android:layout_marginBottom="164dp"
        android:rotation="270"
        android:src="@drawable/line2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tree"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toEndOf="@+id/selfAwareness" />

    <Button
        android:id="@+id/selfAwarenessBtn"
        android:layout_width="118dp"
        android:layout_height="40dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/white_round_button_background"
        android:text="Self-Awareness"
        android:textAllCaps="false"
        android:textColor="#1f314a"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.61"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/mindBtn"
        android:layout_width="118dp"
        android:layout_height="40dp"
        android:layout_marginBottom="308dp"
        android:background="@drawable/white_round_button_background"
        android:text="Mind-Mapping"
        android:textAllCaps="false"
        android:textColor="#1f314a"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/personalBtn"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/white_round_button_background"
        android:text="Personal-Branding"
        android:textAllCaps="false"
        android:textColor="#1f314a"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.697"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

